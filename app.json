{
  "expo": {
    "name": "Bomoko app",
    "slug": "bomoko-app",
    "privacy": "public",
    "android": {
      "package": "com.wezalab.bomoko"
    },
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android"],
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/login1_bg.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "assetExts": ["ttf"]
    }
  }
}
